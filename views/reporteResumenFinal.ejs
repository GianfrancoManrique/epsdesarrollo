<% include partials/head.ejs %>
<title>EMAPAVIGS - REPORTE MAPA</title>

<body class="reporte-mapa">
<div class="wrapper">

    <div class="container">

        <h3 class="page-title">EMAPAVIGS REPORTE - <span> RESUMEN FINAL</span></h3>

        <% if(usuario.tipo.trim()=='S'){ %>

        <h3 class="hour">Consulta al:<span> </span></h3>


        <table style="text-align: center" class="table table-striped table-bordered table-responsive-lg table-responsive-md table-sm">
            <thead  class="bg-primary" style="color: #fff; ">
            <tr style="border-radius: 10px">
                <th>TOTAL DE PREDIOS</th>
                <th>TOTAL AVANCE</th>
                <th>% DE AVANCE</th>

            </tr>
            </thead>
            <tbody >

            <tr>
                <td><%= resumenFinal[0].total_predios %></td>
                <td><%= resumenFinal[0].total_avance %></td>
                <td><%= resumenFinal[0].porc_avance %> %</td>

            </tr>

            </tbody>

        </table>

        <% } else { %>
            <h2>Reporte no autorizado.</h2>
        <% } %>

    </div>
</div>

<% include partials/footer %>

<script>


    var hr = new Date();

    var time =  hr.getHours() + ":" + hr.getMinutes();


    var d = new Date();

    var month = d.getMonth()+1;
    var day = d.getDate();

    var output = ((''+day).length<2 ? '0' : '') + day + '/' +
        ((''+month).length<2 ? '0' : '') + month + '/' +
         d.getFullYear();



    $(".hour span").append( output + ' ' + time );
</script>